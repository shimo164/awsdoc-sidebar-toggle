{
  "manifest_version": 3,
  "name": "AWS Docs Sidebar Toggler",
  "version": "1.0",
  "description": "Automatically toggles the sidebars on AWS documentation pages.",
  "permissions": [
    "tabs",
    "scripting",
		"storage"
  ],
	"options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://docs.aws.amazon.com/*"
  ],
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	}
}
